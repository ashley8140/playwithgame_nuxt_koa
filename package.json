        "mongoose": "^5.5.2",
        "nuxt": "^2.4.0",
        "passport-local": "^1.0.0",
        "sass-loader": "^7.1.0",
        "swiper": "^4.5.0",
        "vue-awesome-swiper": "^3.1.3",
